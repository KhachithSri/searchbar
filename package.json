{
  "name": "basic-searchbar-implementation-using-api",
  "version": "1.0.0",
  "private": true,
  "description": "Monorepo for frontend and backend",
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev:frontend": "npm run dev --workspace frontend",
    "dev:backend": "npm start --workspace backend",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\""
  },
  "dependencies": {
    "concurrently": "^8.0.0"
  }
}
